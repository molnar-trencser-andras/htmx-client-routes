{
  "name": "htmx-client-routes",
  "version": "1.0.1",
  "description": "Client-side routing extension for HTMX",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/npm-package/index.d.ts",
  "unpkg": "dist/htmx-client-routes.min.js",
  "jsdelivr": "dist/htmx-client-routes.min.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build-npm": "rimraf dist && tsc --project tsconfig.npm.json && rollup -c",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest run",
    "test-e2e": "cypress run",
    "prepublishOnly": "pnpm run build-npm"
  },
  "dependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "peerDependencies": {
    "htmx.org": "^2.0.0"
  },
  "keywords": [
    "htmx",
    "routing",
    "client-side",
    "extension"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react-swc": "^4.0.1",
    "cypress": "^15.0.0",
    "eslint": "^9.34.0",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^17.2.0",
    "htmx.org": "^2.0.6",
    "rimraf": "^6.0.1",
    "rollup": "^4.50.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.41.0",
    "vite": "^7.1.4",
    "vitest": "^3.2.4"
  }
}
